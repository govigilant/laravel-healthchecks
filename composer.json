{
    "name": "govigilant/laravel-healthchecks",
    "description": "A Laravel package to add healthchecks which can be integrated with govigilant.io",
    "keywords": [
        "vigilant",
        "laravel",
        "healthchecks"
    ],
    "homepage": "https://govigilant.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Bean",
            "email": "vincent@govigilant.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "govigilant/vigilant-healthchecks-base": "^1.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^10.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpunit/phpunit": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "Vigilant\\LaravelHealthchecks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigilant\\LaravelHealthchecks\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "archive": {
        "exclude": [
            "devenv",
            "art"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vigilant\\LaravelHealthchecks\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan",
        "style": "pint --test",
        "quality": [
            "@test",
            "@analyse",
            "@style"
        ],
        "fix-style": "pint"
    }
}
